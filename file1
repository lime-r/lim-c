from selenium import webdriver
from selenium.webdriver.common.keys import Keys

# 指定浏览器驱动路径
driver_path = './chromedriver.exe' # 可根据实际情况自行修改
browser = webdriver.Chrome(executable_path=driver_path)

# 打开目标网页
url = 'https://www.example.com' # 可替换为您需要输入的网址
browser.get(url)

# 定位网页特定位置并输入内容
input_box = browser.find_element_by_id('input_box_id') # 以id属性来定位输入框，可根据网页元素自行修改
input_box.clear() # 首先清空文本框中的内容
input_box.send_keys("您需要输入的内容") # 向文本框中输入指定内容
# 定位元素位置
element = browser.find_element_by_xpath('//div[@class="example"]')  # 可替换为元素的xpath或其他属性

# 创建ActionChains对象
actions = ActionChains(browser)

# 将鼠标移动到元素位置并进行单击
actions.move_to_element(element).click().perform()

# 关闭浏览器
browser.close()
